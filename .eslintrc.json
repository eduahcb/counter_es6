{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard",
        "prettier",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "globals": {
      "sessionStorage": true
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error"
    }
}
